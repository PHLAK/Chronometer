{
    "name": "phlak/chronometer",
    "description": "Time things",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/PHLAK"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/ChrisKankiewicz"
        }
    ],
    "support": {
        "issues": "https://github.com/PHLAK/Chronometer/issues"
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "php": ">=7.2",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phlak/coding-standards": "^1.2",
        "psy/psysh": "^0.10 || ^0.11",
        "vimeo/psalm": "^4.7",
        "yoast/phpunit-polyfills": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Chronometer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Chronometer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
