{
    "name": "phlak/chronometer",
    "description": "Time things",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^6.0 || ^7.0",
        "psy/psysh": "^0.9.0"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Chronometer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Chronometer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
