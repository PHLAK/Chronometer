{
    "name": "phlak/chronometer",
    "description": "Time things",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/PHLAK"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/ChrisKankiewicz"
        }
    ],
    "support": {
        "issues": "https://github.com/PHLAK/Chronometer/issues"
    },
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phlak/coding-standards": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "psy/psysh": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Chronometer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Chronometer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
